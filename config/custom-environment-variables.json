      "sslEnabled": "S3_DEST_SSL_ENABLED",
      "maxAttempts": "S3_DEST_MAX_ATTEMPTS"
    }
  },
  "NFS": {
    "source": {
      "pvPath": "PV_SOURCE_PATH"
    },
    "destination": {
      "pvPath": "PV_DEST_PATH"
    }
  },
  "worker": {
    "provider": {
      "source": "PROVIDER_FROM",
      "destination": "PROVIDER_TO"
    },
    "job": {
      "type": "JOB_TYPE"
    },
    "task": {
      "batches": "TASK_BATCHES",
      "type": "TASK_TYPE"
    },
    "waitTime": "WORKER_WAIT_TIME",
    "maxRetries": "WORKER_MAX_RETRIES"
