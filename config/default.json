{
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "tracing": {
      "enabled": false,
      "url": "http://telemetryUrl/tracing"
    },
    "metrics": {
      "enabled": false,
      "url": "http://telemetryUrl/metrics",
      "interval": "3"
    }
  },
  "jobManager": {
    "url": "http://127.0.0.1:8080"
  },
  "heartbeat": {
    "url": "http://127.0.0.1:8082",
    "waitTime": 3000
  },
  "S3": {
    "source": {
      "accessKeyId": "vector",
      "secretAccessKey": "vectorPassword",
      "endpointUrl": "http://10.8.0.9:9000",
      "bucket": "3dtilesint",
      "region": "us-east-1",
      "forcePathStyle": true,
      "sslEnabled": false,
      "maxAttempts": 3,
      "sigVersion": "v4"
    },
    "destination": {
      "accessKeyId": "vector",
      "secretAccessKey": "vectorPassword",
      "endpointUrl": "http://10.8.0.9:9000",
      "bucket": "3dtiles-dest",
      "region": "us-east-1",
      "forcePathStyle": true,
      "sslEnabled": false,
      "maxAttempts": 3,
      "sigVersion": "v4"
    }
  },
  "NFS": {
    "source": {
      "pvPath": "/home/netanel/3DModels"
    },
    "destination": {
      "pvPath": "/home/netanel/destination3DModels"
    }
  },
  "fileSyncer": {
    "provider": {
      "source": "S3",
      "destination": "S3"
    },
    "job": {
      "type": "Ingestion_New_3D"
    },
    "task": {
      "type": "Ingestion",
      "maxAttempts": 3
    },
    "waitTime": 5000,
    "maxRetries": 3,
    "intervalMs": 5000,
    "taskPoolSize": 30
  }
}
