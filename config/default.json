{
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "tracing": {
      "enabled": true,
      "url": "http://localhost:4318/v1/traces"
    },
    "metrics": {
      "enabled": true,
      "url": "http://localhost:4318/v1/metrics",
      "buckets": [0.005, 0.01, 0.025, 0.05, 0.1, 0.25, 0.5, 1, 2.5, 5, 10, 15, 50, 250, 500]
    }
  },
  "server": {
    "port": "8080"
  },
  "jobManager": {
    "url": "http://127.0.0.1:8080",
    "job": {
      "type": "Ingestion_New_3D"
    },
    "task": {
      "type": "tilesCopying",
      "pollingIntervalTime": 5000,
      "maxAttempts": 3
    }
  },
  "heartbeat": {
    "url": "http://127.0.0.1:8080",
    "pingingIntervalTime": 3000
  },
  "provider": {
    "source": {
      "kind": "NFS",
      "pvPath": "/Path/To/Models"
    },
    "dest": {
      "kind": "s3",
      "credentials": {
        "accessKeyId": "accessKeyId",
        "secretAccessKey": "secret"
      },
      "endpoint": "http://s3-domain/",
      "bucketName": "3dtiles",
      "region": "region",
      "forcePathStyle": true,
      "maxAttempts": 3,
      "storageClass": "STANDARD"
    }
  },
  "fileSyncer": {
    "intervalMs": 5000,
    "taskPoolSize": 1
  }
}
