{
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "tracing": {
      "enabled": true,
      "url": "http://localhost:4318/v1/traces"
    },
    "metrics": {
      "enabled": true,
      "url": "http://localhost:4318/v1/metrics",
      "buckets": [0.005, 0.01, 0.025, 0.05, 0.1, 0.25, 0.5, 1, 2.5, 5, 10, 15, 50, 250, 500]
    }
  },
  "server": {
    "port": "8087"
  },
  "jobManager": {
    "url": "http://127.0.0.1:8080"
  },
  "heartbeat": {
    "url": "http://127.0.0.1:8082",
    "waitTime": 3000
  },
  "provider": {
    "source": {
      "type": "NFS",
      "accessKeyId": "minioadmin",
      "secretAccessKey": "minioadmin",
      "endpointUrl": "http://127.0.0.1:9000",
      "bucket": "3dtiles",
      "region": "us-east-1",
      "forcePathStyle": true,
      "sslEnabled": false,
      "maxAttempts": 3,
      "sigVersion": "v4"
    },
    "dest": {
      "type": "S3",
      "accessKeyId": "minioadmin",
      "secretAccessKey": "minioadmin",
      "endpointUrl": "http://127.0.0.1:9000",
      "bucket": "3dtiles",
      "region": "us-east-1",
      "forcePathStyle": true,
      "sslEnabled": false,
      "maxAttempts": 3,
      "sigVersion": "v4",
      "storageClass": "STANDARD"
    }
  },
  "fileSyncer": {
    "job": {
      "type": "Ingestion_New_3D"
    },
    "task": {
      "type": "Ingestion",
      "maxAttempts": 3
    },
    "waitTime": 5000,
    "maxRetries": 3,
    "intervalMs": 5000,
    "taskPoolSize": 1
  }
}
